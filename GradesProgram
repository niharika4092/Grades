#include<stdio.h>
int main()
{
int marks;
printf("Enter the marks:");
scanf("%d", &marks);
if(marks<0 || marks>100)
{
printf("\nWrong entry\n");
}
else if(marks<40)
{
printf("\ngrade F\n");
}
else if(marks>=40 && marks<54)
{
printf("\ngrade D\n");
}
else if(marks>=55 && marks<69)
{
printf("\ngrade C\n");
}
else if(marks>=70 && marks<84)
{
printf("\ngrades B\n");
}
else
{
printf("grande A");
}
}
